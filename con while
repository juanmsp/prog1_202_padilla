n1=num=int(input("ingrese numero"))
n2=num=int(input("ingrese numero"))
mayot=0
menor=0

if n1<n2:
    menor,mayor=n1,n2
else: 
    menor;mayor=n2,n1
cont=0
rta=menor
while rta<=mayor:
    rta+=menor
    cont+=1
print(f"se sumo {cont} veces")   
